{
  "name": "universalprofile.cloud",
  "packageManager": "yarn@4.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:{eslint,css,fmt,types,biome}\"",
    "lint:eslint": "eslint --ext '.js,.vue,.ts' .",
    "lint:eslint:fix": "eslint --ext '.js,.vue,.ts' . --fix",
    "lint:css": "stylelint \"**/*.{scss,css,vue}\"",
    "lint:css:fix": "stylelint \"**/*.{scss,css,vue}\" --fix",
    "lint:fmt": "prettier --check --log-level=warn .",
    "lint:fmt:fix": "prettier --write .",
    "lint:types": "tsc --noEmit --project tsconfig.json",
    "lint:biome": "biome check .",
    "lint:biome:fix": "biome check . --apply",
    "prepare": "npm-run-all nuxt:prepare typechain && husky install",
    "yata-fetch": "dotenv -- yata-fetch --config .yata.json",
    "test": "npm-run-all test:unit:ci test:e2e",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:unit": "dotenv -- vitest",
    "test:unit:ui": "dotenv -- vitest --ui",
    "test:unit:ci": "dotenv -- vitest run",
    "test:debug": "npx playwright test --debug",
    "pages:build": "cross-env NODE_ENV=production NITRO_PRESET=cloudflare_pages nuxt build --standalone",
    "nuxt:prepare": "nuxt prepare",
    "typechain": "typechain --target web3-v1 --out-dir contracts/ './shared/abis/LSP2FetcherWithMulticall3.json' && typechain --target web3-v1 --out-dir contracts/ './node_modules/@lukso/lsp-smart-contracts/artifacts/*.json'"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0",
