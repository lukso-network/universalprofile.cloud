    "@types/randombytes": "2.0.3",
    "@types/three": "^0.162.0",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vitest/ui": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "assert": "^2.1.0",
    "bignumber.js": "^9.1.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
