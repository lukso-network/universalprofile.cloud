    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sass": "1.71.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "stylelint": "16.2.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-prettier": "5.0.0",
    "stylelint-scss": "6.2.1",
    "swiper": "~11.1.4",
    "tailwind-variants": "^0.2.1",
    "three": "^0.162.0",
    "typechain": "^8.3.2",
    "typescript": "5.3.3",
    "util": "0.12.5",
    "vitest": "^1.6.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "web3": "~1.10.4",
    "web3-bzz": "1.10.4",
    "web3-utils": "~1.10.4",
    "web3-validator": "^2.0.4",
    "yata-fetch": "2.1.5"
  },
  "resolutions": {
    "@tanstack/query-core": "^5.20.5",
    "web3-bzz": "~1.10.3",
    "web3-shh": "~1.10.3",
    "web3": "~1.10.3",
    "swiper": "~11.1.4"
  },
  "lint-staged": {
    "*.{ts,js,vue,json,yaml,yml}": [
      "biome check",
      "prettier --list-different",
      "eslint"
    ],
    "*.json": "prettier --list-different",
    "*.ya?ml": "prettier --list-different",
    "*.{scss,css,vue}": [
      "biome check",
      "prettier --list-different",
      "stylelint"
    ]
  },
