{
  "name": "wallet.universalprofile.cloud",
  "packageManager": "yarn@4.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:{eslint,css,fmt,types}\"",
    "lint:eslint": "eslint --ext '.js,.vue,.ts' .",
    "lint:eslint:fix": "eslint --ext '.js,.vue,.ts' . --fix",
    "lint:css": "stylelint \"**/*.{scss,css,vue}\"",
    "lint:css:fix": "stylelint \"**/*.{scss,css,vue}\" --fix",
    "lint:fmt": "prettier --check --log-level=warn .",
    "lint:fmt:fix": "prettier --write .",
    "lint:types": "tsc --noEmit --project tsconfig.json",
    "prepare": "npm-run-all nuxt:prepare typechain && husky install",
    "yata-fetch": "dotenv -- yata-fetch --config .yata.json",
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:debug": "npx playwright test --debug",
    "pages:build": "cross-env NODE_ENV=production NITRO_PRESET=cloudflare_pages nuxt build --standalone",
    "nuxt:prepare": "nuxt prepare",
    "typechain": "typechain --target web3-v1 --out-dir types/contracts/ './node_modules/@lukso/lsp-smart-contracts/artifacts/*.json'"
  },
  "devDependencies": {
    "@erc725/erc725.js": "0.22.0",
    "@esbuild-plugins/node-globals-polyfill": "0.2.3",
    "@formatjs/intl": "^2.9.9",
    "@lukso/lsp-smart-contracts": "0.14.0",
    "@lukso/lsp-smart-contracts-12": "npm:@lukso/lsp-smart-contracts@0.12.1",
    "@lukso/web-components": "1.56.0",
    "@nuxtjs/algolia": "^1.10.1",
    "@nuxtjs/device": "^3.1.1",
    "@nuxtjs/plausible": "^0.2.4",
    "@nuxtjs/tailwindcss": "^6.10.3",
    "@pinia-orm/nuxt": "^1.7.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@playwright/test": "1.40.1",
    "@sentry/integrations": "^7.91.0",
    "@sentry/types": "^7.91.0",
    "@sentry/vite-plugin": "^2.10.2",
    "@sentry/vue": "^7.91.0",
    "@typechain/web3-v1": "^6.0.7",
    "@types/fs-extra": "11.0.4",
    "@types/node": "^20.10.6",
    "@types/randombytes": "2.0.3",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "6.0.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-playwright": "0.21.0",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-vue": "9.19.2",
    "ethereum-blockies-base64": "^1.0.2",
    "fs-extra": "11.2.0",
    "https-browserify": "1.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "nuxt": "^3.9.0",
    "pinia": "^2.1.7",
    "pinia-orm": "^1.7.2",
    "postcss-custom-properties": "^13.3.4",
    "postcss-html": "^1.5.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sass": "1.69.7",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "stylelint": "16.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-prettier": "5.0.0",
    "stylelint-scss": "6.0.0",
    "typechain": "^8.3.2",
    "typescript": "5.3.3",
    "util": "0.12.5",
    "vue": "^3.4.6",
    "web3": "~1.10.3",
    "web3-utils": "~1.10.3",
    "yata-fetch": "2.1.5"
  },
  "resolutions": {
    "web3-bzz": "~1.10.3",
    "web3-shh": "~1.10.3",
    "web3": "~1.10.3"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "prettier --list-different",
      "eslint"
    ],
    "*.json": "prettier --list-different",
    "*.ya?ml": "prettier --list-different",
    "*.{scss,css,vue}": [
      "prettier --list-different",
      "stylelint"
    ]
  },
  "version": "1.0.0"
}
